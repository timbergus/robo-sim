    "node-sass": "^4.14.1",
    "nyc": "^7.0.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "tap-spec": "^4.1.1",
