{
  "name": "robo-sim",
  "version": "1.0.0",
  "description": "This is a johnny-five test.",
  "main": "src/app/main.js",
  "scripts": {
    "test": "nyc --reporter=lcov node test.js | tap-spec",
    "start": "npm run copy-files && webpack-dev-server --env.dev",
    "build": "npm run copy-files && webpack --env.dev",
    "build:prod": "npm run copy-files && webpack --env.prod -p",
    "copy-files": "copyfiles -u 1 src/images/** dist",
    "validate": "npm run validate-webpack:dev && npm run validate-webpack:prod",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "hw": "node src/hardware/main.js",
    "hw:dev": "nodemon src/hardware/main.js"
  },
  "author": "Gustavo Mu√±oz <timbergus@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "cors": "^2.7.1",
    "express": "^4.14.0",
    "johnny-five": "^0.9.59",
    "material-ui": "^0.15.2",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.5.2",
    "react-tap-event-plugin": "^1.0.0",
    "socket.io": "^1.4.8",
    "superagent": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
